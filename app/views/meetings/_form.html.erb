<%= render "layouts/errors", :object => @meeting %>

<%= form_for [@place, @meeting] do |f| %>
  <%=f.label :date %>

  <%= f.text_field :date, "data-provide" => 'datepicker' %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :start_time, "From" %>
    <%= f.time_field :start_time %>
  </div>

  <div class="field">
    <%= f.label :end_time, "To" %>
    <%= f.time_field :end_time %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
